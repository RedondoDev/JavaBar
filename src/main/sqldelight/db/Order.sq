import java.time.LocalDate;

CREATE TABLE OrderDB (
    id INTEGER PRIMARY KEY,
    date TEXT AS LocalDate NOT NULL,
    time TEXT NOT NULL,
    amount REAL NOT NULL,
    waiter TEXT NOT NULL
);

selectAll:
SELECT *
FROM OrderDB;

maxId:
SELECT MAX(id)
FROM OrderDB;

insert:
INSERT OR IGNORE INTO OrderDB (id, date, time, amount, waiter)
VALUES (?, ?, ?, ?, ?);

delete:
DELETE FROM OrderDB
WHERE id = ?;